{
  "name": "webnwb",
  "version": "0.0.12",
  "description": "A JavaScript API for working with Neurodata stored in the NWB Format",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": {
    "dist/index.js": "dist/index.js",
    "dist/index.esm.js": "dist/index.esm.js"
  },
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tinybuild",
    "docs": "cd docs && yarn && yarn run start",
    "typecheck": "tsc --watch --preserveWatchOutput --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c && tsc",
    "build:docs": "cd docs && yarn && yarn run build"
  },
  "keywords": [
    "neuroscience",
    "data"
  ],
  "repository": "https://github.com/brainsatplay/WebNWB.git",
  "author": "Garrett Flynn <gflynn@usc.edu>",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "dependencies": {
    "hdf5-io": "^0.0.4",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "lit": "^2.6.1",
    "resize-observer-polyfill": "^1.5.1",
    "tinybuild": "^0.3.169",
    "webgl-plot": "^0.7.0"
  },
  "peerDependencies": {
    "h5wasm": ">= 0.2.2 < 1"
  },
  "peerDependenciesMeta": {
    "h5wasm": {
      "autoinstall": true
    }
  }
}
