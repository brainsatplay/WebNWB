{
  "name": "jsnwb",
  "version": "0.0.1",
  "description": "A JavaScript API for working with Neurodata stored in the NWB Format",
  "main": "./dist/index.js",
  "module": "./dist/module.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cd docs && yarn && yarn run start",
    "typecheck": "tsc --watch --preserveWatchOutput --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c && tsc",
    "build:docs": "cd docs && yarn && yarn run build"
  },
  "keywords": [
    "neuroscience",
    "data"
  ],
  "repository": "https://github.com/brainsatplay/jsnwb.git",
  "author": "Garrett Flynn <gflynn@usc.edu>",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-wasm": "^5.1.2",
    "@web/rollup-plugin-copy": "^0.3.0",
    "babel-loader": "^8.2.3",
    "rollup": "^2.67.2",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "h5wasm": "^0.1.8"
  }
}
