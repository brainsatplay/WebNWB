{
  "name": "webnwb",
  "version": "0.1.0",
  "description": "A JavaScript API for working with Neurodata stored in the NWB Format",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.es.js",
    "default": "./dist/index.umd.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "docs": "cd docs && yarn && yarn run start",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "neuroscience",
    "data"
  ],
  "repository": "https://github.com/brainsatplay/WebNWB.git",
  "author": "Garrett Flynn <garrettmflynn@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "esconform": "0.0.6",
    "hdf5-io": "0.0.26",
    "lit": "^2.6.1",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "4.9.5",
    "visualscript": "0.0.11",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vitepress": "^1.0.0-alpha.73",
    "vitest": "^0.30.1",
    "webgl-plot": "^0.7.0",
    "yaml": "^2.1.1"
  }
}
